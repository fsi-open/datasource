{
    "name": "fsi/datasource",
    "type": "library",
    "description": "FSi DataSource Component",
    "keywords": ["component", "datasource", "fsi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Szczepan Cieslik",
            "email": "szczepan@fsi.pl"
        },
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "symfony/event-dispatcher": "^3.4|^4.0",
        "symfony/options-resolver": "^3.4|^4.0|^5.0",
        "fsi/data-indexer" : "0.9.*|^1.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "doctrine/persistence": "^1.3|^2.0",
        "phpstan/phpstan": "^0.12.66",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^7.5|^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Component\\DataSource\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\Component\\DataSource\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.3": "1.3-dev",
            "1.2": "1.2-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
